{
    "name": "dleno/hyperf-env-multi",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "php",
        "hyperf"
    ],
    "description": "",
    "autoload": {
        "psr-4": {
            "Dleno\\HyperfEnvMulti\\": "src/"
        }
    },
    "require": {
        "php": ">=7.2",
        "ext-swoole": ">=4.4"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.14",
        "hyperf/testing": "^2.2",
        "hyperf/di": "^2.2",
        "phpstan/phpstan": "^0",
        "vlucas/phpdotenv": "^5.2",
        "hyperf/config":  "^2.2"
    },
    "config": {
        "sort-packages": true
    },
    "scripts": {
        "test": "co-phpunit -c phpunit.xml --colors=always",
        "analyse": "phpstan analyse --memory-limit 300M -l 0 ./src",
        "cs-fix": "php-cs-fixer fix $1"
    },
    "extra": {
        "hyperf": {
            "config": "Dleno\\HyperfEnvMulti\\ConfigProvider"
        }
    }
}
